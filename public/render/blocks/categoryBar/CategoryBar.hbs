<div class="{{classes}}">
    {{{LeftButton}}}{{{RightButton}}}
        <ul class="category-bar__list">
            {{#each categories}}
                <li class="category-bar__li">
                    {{{this}}}
                </li>
            {{else}}
                <p class="message">Здесь должны быть категории</p>
            {{/each}}
        </ul>
</div>

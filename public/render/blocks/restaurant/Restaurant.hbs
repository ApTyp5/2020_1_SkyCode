<a href="{{href}}" class="restaurant-list__restaurant {{classes}}">
    <div class="restaurant__image">
        {{{img}}}
        <span class="restaurant__avg-time">
            {{avgDeliveryMinTime}} - {{avgDeliveryMaxTime}} мин
        </span>
    </div>
    <span class="restaurant__name">{{name}}</span>
    <br/>
    <span class="restaurant__rate">{{{rateImg}}}{{rate}}</span>
</a>
